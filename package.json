{
  "name": "portfolio_web",
  "version": "1.0.0",
  "description": "portfolio_web",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass-chokidar -w ./sass -o ./css --watch  --recursive",
    "_watch:serve": "live-server ./",
    "dev": "npm-run-all --parallel watch:**",
    "browser-sync": "browser-sync start --server ./ --files ./",
    "auto-prefixer": "postcss *.css --use autoprefixer -d build/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.0.1",
    "live-server": "^1.2.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.1.0",
    "sass": "^1.27.1"
  }
}
